{
	"discovery": "https://accounts.google.com/.well-known/openid-configuration",
	"authRequest": {
		"scopes": ["openid"]
	},
	"redirectUri": "https://example.com:443/oidc/callback",
	"redis": {
		"session": {
			"name": "op.sid",
			"resave": false,
			"saveUninitialized": true,
			"cookie": {
				"sameSite": false,
				"secure": true
			}
		}
	}
}
